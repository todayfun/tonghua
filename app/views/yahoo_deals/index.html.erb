<h1>Listing yahoo_deals</h1>

<table>
  <tr>
    <th>Code</th>
    <th>Open</th>
    <th>Close</th>
    <th>High</th>
    <th>Low</th>
    <th>Volume</th>
    <th>Adj</th>
    <th>On</th>
    <th></th>
  </tr>

<% @yahoo_deals.each do |yahoo_deal| %>
  <tr>
    <td><%= yahoo_deal.code %></td>
    <td><%= yahoo_deal.open %></td>
    <td><%= yahoo_deal.close %></td>
    <td><%= yahoo_deal.high %></td>
    <td><%= yahoo_deal.low %></td>
    <td><%= yahoo_deal.volume %></td>
    <td><%= yahoo_deal.adj %></td>
    <td><%= yahoo_deal.on %></td>
    <td><%= link_to 'Show', yahoo_deal %></td>   
  </tr>
<% end %>
</table>

<br />
<%= will_paginate @yahoo_deals %>
<br />
<%= "total: #{@yahoo_deals.total_entries}" %>
<br />
<%= link_to 'New Yahoo deal', new_yahoo_deal_path %>
